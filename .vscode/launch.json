{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch SSR Webpack Program",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal",
            "stopOnEntry": true,
            "autoAttachChildProcesses": true,
        }
    ]
}